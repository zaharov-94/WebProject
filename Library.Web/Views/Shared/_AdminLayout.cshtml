<!DOCTYPE html>
<!-- saved from url=(0053)https://getbootstrap.com/docs/3.3/examples/dashboard/ -->
<html lang="en">
<head>
    @Styles.Render("~/Content/justified-nav.css")
    @Scripts.Render("~/Scripts/jquery.min.js")
</head>
<body>
    <nav class="navbar navbar-fixed-top navbar-inverse">
            <div class="navbar navbar-right">
                @Html.Partial("_LoginPartial")
            </div>
            <div class="collapse navbar-collapse">
                <ul class="navbar-nav">
                    <li class="nav-item active">
                        @Html.ActionLink("Books", "Index", "Book", new { @class = "nav-link" })
                    </li>
                    <li class="nav-item">
                        @Html.ActionLink("Magazines", "Index", "Magazine")
                    </li>
                    <li class="nav-item">
                        @Html.ActionLink("Brochures", "Index", "Brochure")
                    </li>
                    <li class="nav-item">
                        @Html.ActionLink("Publication houses", "Index", "PublicationHouse")
                    </li>
                    <li class="nav-item">
                        @Html.ActionLink("All Publications", "Index", "AllPublication")
                    </li>
                </ul>
            </div>
    </nav>
    <div class="container body-content body-admin">
        @RenderBody()
    </div>
    @Scripts.Render("~/Scripts/popper.min.js")
    @Scripts.Render("~/Scripts/bootstrap.min.js")
</body>
</html>